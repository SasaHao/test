package com.letv.ssp.pages;

import org.openqa.selenium.By;

public class LoginPage {
	/**
	 * 登录页面各元素
	 */
	public By username = By.name("username");
	public By password = By.name("password");
	public By submit = By.className("btn-inverse");
	public By logout = By.partialLinkText("退出");
	public By store_username = By.name("loginname");
	public By store_password = By.id("password");
	public By store_submit = By.id("login-btn");
}
